<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logoapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LogoApplication">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SubSectionActivity" />
        <activity android:name=".activity.ExercisesActivity" />
        <activity android:name=".activity.TaskActivity" />
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.ProfileActivity"/>
        <activity android:name=".activity.EditProfileActivity"/>
        <activity android:name=".activity.ChatListActivity"/>
        <activity android:name=".activity.ChatActivity"/>
        <activity android:name=".activity.RegisterActivity"/>
        <activity android:name=".activity.RegisterTypeActivity"/>
        <activity android:name=".activity.UserLIstActivity" />
    </application>

</manifest>